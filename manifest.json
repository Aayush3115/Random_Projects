{
  "manifest_version": 3,
  "name": "Random Quote Generator",
  "description": "Get a random quote with one click.",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "storage"
  ]
}


